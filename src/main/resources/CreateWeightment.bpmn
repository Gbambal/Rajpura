        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[
try {
    Object dbDataResult = kcontext.getVariable("secondtoleranceQueryResp");
    if (dbDataResult != null) {
        if (dbDataResult instanceof java.util.ArrayList<?>) {
            java.util.ArrayList<?> dbresultList = (java.util.ArrayList<?>) dbDataResult;
            for (Object dbelements : dbresultList) {
                String[] dbcolumn = dbelements.toString().split(",");

                 String dbtoleranceTypegw = dbcolumn[11];
                System.out.println("--------------view: dbtoleranceTypegw--" + dbtoleranceTypegw);
               //context.setVariable("toleranceType", dbtoleranceTypegw);

                
                String dbminimumAlertgw = dbcolumn[13];
                System.out.println("--------------view: dbminimumAlertgw--" + dbminimumAlertgw);
                kcontext.setVariable("dbminimumAlertgw", dbminimumAlertgw);

                String dbmaximumAlertgw = dbcolumn[14];
                System.out.println("--------------view: dbmaximumAlertgw--" + dbmaximumAlertgw);
				
				String dbminimumAlertgw2 = dbcolumn[15];
                System.out.println("--------------view: dbminimumAlertgw2--" + dbminimumAlertgw2);
                kcontext.setVariable("dbminimumAlertgw2", dbminimumAlertgw2);

                String dbmaximumAlertgw2 = dbcolumn[16];
                System.out.println("--------------view: dbmaximumAlertgw2--" + dbmaximumAlertgw2);
				
                String dbapprovalTolerance = dbcolumn[17];
                System.out.println("--------------view: approvalTolerance--" + dbapprovalTolerance);
				
                String dbtoleranceTypetw = dbcolumn[19];
                System.out.println("--------------view: toleranceType--" + dbtoleranceTypetw);
               

                
                String dbminimumAlerttw = dbcolumn[21];
                System.out.println("--------------view: dbminimumAlerttw--" + dbminimumAlerttw);
                

                String dbmaximumAlerttw = dbcolumn[22];
                System.out.println("--------------view: dbmaximumAlerttw--" + dbmaximumAlerttw);
				
				
                String dbdiqty = dbcolumn[9];
                System.out.println("--------------view: dbdiqty--" + dbdiqty);
				double diQty = Double.parseDouble(dbdiqty);
				
                String tareweight = dbcolumn[24];
                System.out.println("--------------view: tw--" + tareweight);
				double tw = Double.parseDouble(tareweight);
				
                //kcontext.setVariable("weightType", dbweightType);
				//String weightType = "TW";
				double GW = Double.valueOf(weight);	
                System.out.println("Get DB data completed successfully");
				if("TW".equalsIgnoreCase(weightType)) {
				System.out.println("no ;ogtc for tw " );
}
				
				else{
                    float approvalToleranceGW =  Float.parseFloat(dbapprovalTolerance);
				    kcontext.setVariable("approvalTolerancee", approvalToleranceGW);
					if ("PERCENTAGE".equalsIgnoreCase(dbtoleranceTypegw)) {
                    double maximumAlert = Double.parseDouble(dbmaximumAlertgw2);
                    double minimumAlert = Double.parseDouble(dbminimumAlertgw2);
                   
				   
				   

                    if ((GW-tw) >= (GW-tw)-minimumAlert*(GW-tw)/100 && (GW-tw) <= (GW-tw)+minimumAlert*(GW-tw)/100) {
                        System.out.println("Within range");
						kcontext.setVariable("isToleranceFailed", 0);
                
                    } else {
                        System.out.println("out of range");
						if((GW-tw) >= (GW-tw)-(GW-tw)+minimumAlert*(GW-tw)/100){
						kcontext.setVariable("tolerancePositive", String.valueOf((GW-tw)+maximumAlert*(GW-tw)/100));
						
						}else{
						kcontext.setVariable("toleranceNegative", String.valueOf((GW-tw)-maximumAlert*(GW-tw)/100));
						}
						kcontext.setVariable("isToleranceFailed", 1);
						double newDiq= ((GW-tw));
						System.out.println("newDiq "+newDiq);
						System.out.println("gross wight "+(GW));
						System.out.println("tare wight "+(tw));
						System.out.println("net wight "+(GW-tw));
						System.out.println("lower "+((GW-tw)-maximumAlert*(GW-tw)/100));
						System.out.println("upper "+(newDiq+maximumAlert*(GW-tw)/100));
						
						
					
                    }
                } else if ("RANGE".equalsIgnoreCase(dbtoleranceTypegw)) {
                    double maximumAlert = Double.parseDouble(dbmaximumAlertgw);
                    double minimumAlert = Double.parseDouble(dbminimumAlertgw);
                   
                   
				   
				   
                    if ((GW-tw) >= (GW-tw) - minimumAlert && (GW-tw) <= (GW-tw) + maximumAlert) {
                        System.out.println("Within the Range");
						kcontext.setVariable("isToleranceFailed", 0);
						
                    } else {
						if((GW-tw) >= (GW-tw) - minimumAlert){
						kcontext.setVariable("tolerancePositive", String.valueOf(diQty*1000+maximumAlert));
						}else{
						kcontext.setVariable("toleranceNegative", String.valueOf(diQty*1000-minimumAlert));
						}
                        System.out.println("Not in Range");
						kcontext.setVariable("isToleranceFailed", 1);
						
                    }
                }
				else if ("KG".equalsIgnoreCase(dbtoleranceTypegw)) {
                    double maximumAlert = Double.parseDouble(dbmaximumAlertgw);
                    double minimumAlert = Double.parseDouble(dbminimumAlertgw);
                  
                   
				   
					if ((GW-tw) >= (GW-tw)-minimumAlert && (GW-tw) <= (GW-tw)+maximumAlert) {
                        System.out.println("Within the range");
						kcontext.setVariable("isToleranceFailed", 0);
                    } else {
					if((GW-tw) >= (GW-tw)-minimumAlert){
					kcontext.setVariable("tolerancePositive", String.valueOf((GW-tw)+maximumAlert));
					}else{
					kcontext.setVariable("toleranceNegative", String.valueOf((GW-tw)-minimumAlert));
					}
                        System.out.println("Not in range");
						kcontext.setVariable("isToleranceFailed", 1);
                    }
                } else {
                    System.out.println("Unknown toleranceType: " + dbtoleranceTypegw);
                   // kcontext.setVariable("responseMsg", "Unknown toleranceType: " + dbtoleranceTypegw);
                }
}
				
				}

}else {
            System.out.println("Tag ID not found");
            //kcontext.setVariable("responseMsg", "Tag ID not found");
        }
}
else {
        System.out.println("No data for Tag ID");
        kcontext.setVariable("responseMsg", "Tag ID not found");
    }
}
catch (Exception e) {
    System.out.println("Exception in getDB data service: " + e.getMessage());
}

