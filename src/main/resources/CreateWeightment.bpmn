      <bpmn2:script><![CDATA[System.out.println("####----------location check for Bulker or GW is----------####");

try {
    if (vhcl_body_type.equals("BULKR")) {
        kcontext.setVariable("validGwBulker", "Y");
        System.out.println("Vehicle is valid for Bulker");
    } else if (vhcl_body_type.equals("BAG") || vhcl_body_type.isEmpty()) {
        kcontext.setVariable("validGwBulker", "N");
        System.out.println("Vehicle is valid for GW");
    } else {
        System.out.println("vhcl_body_type is not matching");
        kcontext.setVariable("validGwBulker", "N");
    }
} catch (NumberFormatException e) {
    System.out.println("Invalid number format: " + e.getMessage());
    kcontext.setVariable("validGwBulker", "N");
}
]]></bpmn2:script>
