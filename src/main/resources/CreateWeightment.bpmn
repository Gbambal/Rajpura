          <drools:script><![CDATA[
System.out.println("----update trip query weightType is: " + weightType);
String updateWbDataquery = "";
try {
    if (weightType.equalsIgnoreCase("TW")) {
        updateWbDataquery = "UPDATE provisionalPlms.trip t JOIN vw_trip_stages ts ON t.tripid = ts.tripid SET t.mapPlantStageLocation = ts.Next_location, t.tareWeight = NOW(), t.tw = '" + weight + "', t.sequence_number = sequence_number + 1 WHERE ts.tag_id = '" + tagId + "'";
    } else {
        updateWbDataquery = "UPDATE provisionalPlms.trip t JOIN vw_trip_stages ts ON t.tripid = ts.tripid SET t.mapPlantStageLocation = 'WB-1 (GW)', t.grossWeight = NOW(), t.gw = '" + weight + "', t.sequence_number = '6' WHERE ts.tag_id = '" + tagId + "'";
    }
    kcontext.setVariable("updateWbTripQuery", updateWbDataquery);
    System.out.println("----update trip query is: " + updateWbDataquery);
} catch (Exception e) {
    System.out.println("Exception during update trip query creation: " + e.getMessage());
    e.printStackTrace();
}
]]></drools:script>
