        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[StringBuilder queryBuilder = new StringBuilder();

if (weightType.equals("TW")) {
    queryBuilder.append("INSERT INTO provisionalPlms.weightmentDetails (tripId, weightType, weight, entryTime, exitTime, weighbridgeCode, weighridgeImg) VALUES ('")
                .append(tripId).append("','").append(weightType).append("','").append(weight).append("','").append(entryTime).append("','").append(exitTime)
                .append("','").append(weighbridgeCode).append("','").append(weighridgeImg).append("')");
} else {
    queryBuilder.append("INSERT INTO provisionalplms.weightmentdetails (tripId, weightType, isToleranceFailed, weight, entryTime, exitTime, weighbridgeCode, weighridgeImg, toleranceActionDateTime) VALUES ('")
    .append(tripId).append("','").append(weightType).append("',").append(isToleranceFailed).append(",'").append(weight).append("','").append(entryTime).append("','").append(exitTime)
    .append("','").append(weighbridgeCode).append("','").append(weighridgeImg).append("', NOW())");
}

String createQuery = queryBuilder.toString();
kcontext.setVariable("weightmentQuery", createQuery);
System.out.println("---create weightment query is :" + createQuery);]]></drools:script>
