String jsonString = resp.toString();

// Use Jackson ObjectMapper to parse JSON
com.fasterxml.jackson.databind.ObjectMapper objectMapper = new com.fasterxml.jackson.databind.ObjectMapper();
try {
    // Parse JSON string to JsonNode
    com.fasterxml.jackson.databind.JsonNode jsonNode = objectMapper.readTree(jsonString);

    // Check if the "message" field exists
    com.fasterxml.jackson.databind.JsonNode messageNode = jsonNode.get("message");
    if (messageNode != null && messageNode.isTextual()) {
        // Extract the "message" field
        String messageValue = messageNode.asText();

        // Print the extracted value
        System.out.println("Message Value: " + messageValue);
        kcontext.setVariable("centralMsgresp", messageValue);
    } else {
        System.out.println("No 'status' field found in the JSON response.");
        // Handle the absence of the "message" field accordingly
    }
} catch (Exception e) {
    e.printStackTrace();
}
kcontext.setVariable("status","SUCCESS");

