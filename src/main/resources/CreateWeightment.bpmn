    <bpmn2:task id="_2E41C9D6-4BB1-46D2-A809-834F8BAD4A0B" drools:taskName="ExecuteSQL" name="BulkerBagCheck">
      <bpmn2:documentation id="_s8mZUKf-Ee-iPvEMn12H1Q"><![CDATA[execute-sql-workitem/index.html]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[BulkerBagCheck]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("Bag / Bulker Check Validation" + tagId);
String bulBagQuery = "SELECT (select CASE WHEN vhcl_body_type = 'BULKR' THEN 'BULKR' ELSE 'BAG' END from provisionalplms.vehiclemaster vv WHERE vv.registration_number = mm.vehicle_number AND vv.plant_code = mm.plantCode) AS vhcl_body_type FROM provisionalplms.vehicletagmapping mm where tag_id = '" + tagId + "'";
kcontext.setVariable("bulkerCheckQuery",bulBagQuery);]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("-----checking Bulker Validation Check------");

try {
    Object dbDataResult = kcontext.getVariable("bulkerCheckQueryResp");

    if (dbDataResult instanceof ArrayList<?>) {
        ArrayList<?> dbresultList = (ArrayList<?>) dbDataResult;

        if (!dbresultList.isEmpty()) {
            Object firstElement = dbresultList.get(0);
            if (firstElement instanceof String) {
                for (Object dbelement : dbresultList) {
                    String[] dbcolumn = dbelement.toString().split(",");
                    String dbbulkerValidation = dbcolumn[0].trim();
                    System.out.println("--------------view: vhcl_body_type------" + dbbulkerValidation);
					kcontext.setVariable("vhcl_body_type",dbbulkerValidation);

                    // Check if tolerance validation
                    if (dbbulkerValidation.equals("BULKR")) {
                        kcontext.setVariable("validBulker", "Y");
                        System.out.println("BULKR is valid for WB Bulker");
                    } else if (dbbulkerValidation.equals("BAG")) {
                        kcontext.setVariable("validBulker", "N");
                        System.out.println("BULKR is not valid for Bag Tolerance");
                    } else {
                        System.out.println("bulkerValidation is not valid ");
                        // kcontext.setVariable("validStatus", "N");
                        kcontext.setVariable("resMsg", "INVLD_BLK");
                    }
                }
            } else {
                System.out.println("Invalid element type in the result list");
                kcontext.setVariable("responseMsg", "Invalid element type in the result list");
            }
        } else {
            System.out.println("No data in the result list");
            kcontext.setVariable("responseMsg", "No data in the result list");
        }
    } else {
        System.out.println("No data or invalid data");
        kcontext.setVariable("responseMsg", "No data or invalid data");
    }
} catch (Exception e) {
    System.err.println("Exception in getDB data service: " + e.getMessage());
    e.printStackTrace();
}]]></drools:script>
        </drools:onExit-script>
