    <bpmn2:task id="_A52D2288-0661-4DA0-980A-58B699D7CCAD" drools:taskName="ExecuteSQL" name="twCheckByTrip">
      <bpmn2:documentation id="_s8mZMqf-Ee-iPvEMn12H1Q"><![CDATA[execute-sql-workitem/index.html]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[twCheckByTrip]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("request tripId for tw" + tripId);
String twQuery = "Select tw from provisionalplms.trip where tripId = '" + tripId + "' and status='A' and grossWeight is null and tw is not null LIMIT 1";
kcontext.setVariable("twQueryCheck", twQuery);
]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("-----VALIDATING TW------");

try {
    Object dbDataResult = kcontext.getVariable("twQueryCheckResp");

    if (dbDataResult instanceof ArrayList<?>) {
        ArrayList<?> dbresultList = (ArrayList<?>) dbDataResult;

        if (!dbresultList.isEmpty()) {
            Object dbelements = dbresultList.get(0);
            if (dbelements instanceof String) {
                String dbtw = dbelements.toString().trim();
                System.out.println("--------------view: tw :" + dbtw);

                if (dbtw != null && !dbtw.isEmpty() && !dbtw.equals("null")) {
                    System.out.println("tw found");
                    kcontext.setVariable("twValue", dbtw); // Store TW value for further processing
                } else {
                    System.out.println("TW is empty or null");
                    kcontext.setVariable("responseMsg", "TW is empty or null");
                }
            } else {
                System.out.println("Unexpected data type in result list");
                kcontext.setVariable("responseMsg", "Unexpected data type in result list");
            }
        } else {
            System.out.println("No data in the result list");
            kcontext.setVariable("responseMsg", "No data in the result list");
        }
    } else {
        System.out.println("No data or invalid data for TW");
        kcontext.setVariable("responseMsg", "No data or invalid data for TW");
    }
} catch (Exception e) {
    System.err.println("Exception in getDB data service: " + e.getMessage());
    e.printStackTrace();
}
]]></drools:script>
        </drools:onExit-script>
